local function find_by_name(t, name) for _, v in pairs(t) do if v.name == name then return v end end end

local actor = find_by_name(getactors(), 'lol')
syn.run_on_actor(actor,[[          
    -- UI
loadstring(game:HttpGet("https://raw.githubusercontent.com/Mick-gordon/Blackout-Script-Development/main/Main%20Files%20for%20the%20Butt "))()
local Ui = loadstring(game:HttpGet("https://raw.githubusercontent.com/Mick-gordon/Blackout-Script-Development/main/Main%20UI"))()
local Ui = Library
local LoadTime = tick()


local Loader = Library.CreateLoader(
    "Blackout Script Development", 
    Vector2.new(300, 300)
)
local Window = Library.Window(
    "Blackout Script Development", 
    Vector2.new(500, 620)
)
Window.Watermark(
    "Blackout Script Development, By Mick Gordon#2226"
)
--Legit tab
local LegitTab = Window:Tab("Legit")
-- Bullet redirect
local SectionBR = LegitTab:Section(
    "Bullet Redirect", 
    "Left"
)
SectionBR:Toggle({
    Title = "Enable", 
    Flag = "EnableBR",
    Callback = function(EBR)
        _G.SilentAim.Enabled=EBR
    end
})
SectionBR:Slider({
    Title = "Fov", 
    Flag = "FovBR", 
    Symbol = "", 
    Default = 300, 
    Min = 0, 
    Max = 800, 
    Decimals = 1,
    Callback = function(BRF)
        _G.SilentAim.Fov=BRF
    end
})
SectionBR:Toggle({
    Title = "Draw Fov", 
    Flag = "ShowFovBR",
    Callback = function(SFBR)
        _G.SilentAim.Show_Fov=SFBR
    end
}):
Colorpicker({
    Color = Library.Theme.Accent[2], 
    Flag = "ShowFovColorBR",
    Callback = function(BRDFC)
        _G.SilentAim.Fov_Color=BRDFC
    end
})
SectionBR:Toggle({
    Title = "Ignore Fov", 
    Flag = "Ignore_fov",
    Callback = function(IFBR)
        _G.SilentAim.Ignore_Fov=IFBR
    end
})
SectionBR:Toggle({
    Title = "Auto Shoot", 
    Flag = "Auto_Shoot",
    Callback = function(ASBR2)
        _G.SilentAim.Auto_Shoot=ASBR2
    end
})
SectionBR:Toggle({
    Title = "Auto Wall", 
    Flag = "Auto_Wall",
    Callback = function(AWBR)
        _G.SilentAim.Auto_Wall=AWBR
    end
})
SectionBR:Dropdown({
    Title = "Hit Scan", 
    List = {"All","Head","Torso","Right Arm","Left Arm","Right Leg","Left Leg"}, 
    Default = "Head", 
    Flag = "BRH",
    Callback = function(BRH1)
        if BRH1=="All" then
            _G.SilentAim.Target=GetR6Parts()
          else
            _G.SilentAim.Target={BRH1}
          end
    end
})

--Aim Assist 
local AimAssistSection = LegitTab:Section(
    "Aim Assist", 
    "Right"
)
AimAssistSection:Toggle({
    Title = "Enable", 
    Flag = "Enable_AimAssist",
    Callback = function(EAA)
        _G.AimAssist.Enabled=EAA
    end
})
AimAssistSection:Toggle({
    Title = "Visibe Check", 
    Flag = "Visibe_Check",
    Callback = function(VBAA)
        _G.AimAssist.VisibleCheck=VBAA
    end
})
AimAssistSection:Toggle({
    Title = "Draw Fov", 
    Flag = "Show_Fov_aimAss",
    Callback = function(SFAA)
        _G.AimAssist.Show_FOV=SFAA
    end
}):
Colorpicker({
    Color = Library.Theme.Accent[2], 
    Flag = "FCAA",
    Callback = function(AADFC1)
        _G.AimAssist.FOV_Color=AADFC1
    end
})
AimAssistSection:Slider({
    Title = "Fov", 
    Flag = "AimAssFov", 
    Symbol = "", 
    Default = 400, 
    Min = 0, 
    Max = 800, 
    Decimals = 1,
    Callback = function(AAF)
        _G.AimAssist.FOV=AAF
    end
})
AimAssistSection:Slider({
    Title = "Smoothing", 
    Flag = "AAS1", 
    Symbol = "", 
    Default = 10, 
    Min = 2, 
    Max = 50, 
    Decimals = 0.5,
    Callback = function(AAS)
        _G.AimAssist.Smoothness=AAS
    end
})
AimAssistSection:Dropdown({
    Title = "Aim Assist Key", 
    List = {"MouseButton1", "MouseButton2"}, 
    Default = "MouseButton2", 
    Flag = "AAK",
    Callback = function(AAK1)
        _G.AimAssist.Keything=AAK1
    end
})
AimAssistSection:Dropdown({
    Title = "Hit Scan", 
    List = {"head", "torso"}, 
    Default = "torso", 
    Flag = "AAH",
    Callback = function(AAH1)
        _G.AimAssist.Target=AAH1
    end
})

--Rage Tab
local RageTab = Window:Tab("Rage")
local GunOptiuons = RageTab:Section(
    "Gun Mods", 
    "Right"
)
GunOptiuons:Toggle({
    Title = "Enable", 
    Flag = "EGM1",
    Callback = function(EGM)
        _G.GunOptins.Enabled=EGM
    end
})
GunOptiuons:Toggle({
    Title = "Instant Animations", 
    Flag = "IGMA",
    Callback = function(IRAGM)
        _G.GunOptins.Animation=IRAGM
        _G.GunOptins.Reset=IRAGM
    end
})
GunOptiuons:Toggle({
    Title = "No Sway", 
    Flag = "NWGM",
    Callback = function(GMNS)
        _G.GunOptins.Sway=GMNS
    end
})
GunOptiuons:Toggle({
    Title = "Static X", 
    Flag = "GMSX",
    Callback = function(SXGM)
        _G.GunOptins.StaticX=SXGM
    end
})
GunOptiuons:Toggle({
    Title = "Static Y", 
    Flag = "GMSY",
    Callback = function(SYGM)
        _G.GunOptins.StaticY=SYGM
    end
})
GunOptiuons:Toggle({
    Title = "Static Z", 
    Flag = "GMSZ",
    Callback = function(SZGM)
        _G.GunOptins.StaticZ=SZGM
    end
})

local PlayerMovement = RageTab:Section(
    "Player Movemnt", 
    "Left"
)
PlayerMovement:Toggle({
    Title = "Enable", 
    Flag = "EPM",
    Callback = function(EPM1)
        _G.PlayerMove.Enabled=EPM1
    end
})
PlayerMovement:Toggle({
    Title = "Jump Power", 
    Flag = "JPE1",
    Callback = function(JPE)
        _G.PlayerMove.JP=JPE
    end
})
PlayerMovement:Slider({
    Title = "Jump Power", 
    Flag = "JPS", 
    Symbol = "", 
    Default = 40, 
    Min = 0, 
    Max = 250, 
    Decimals = 1,
    Callback = function(JPS1)
        _G.PlayerMove.JPV=JPS1
    end
})
PlayerMovement:Toggle({
    Title = "Walk Speed", 
    Flag = "WSPGM",
    Callback = function(WSPM)
        _G.PlayerMove.WS=WSPM
    end
})
PlayerMovement:Slider({
    Title = "Walk Speed", 
    Flag = "JPS23s", 
    Symbol = "", 
    Default = 16, 
    Min = 0, 
    Max = 100, 
    Decimals = 1,
    Callback = function(JPS1)
        _G.PlayerMove.WSV=JPS1
    end
})

-- Visuals Tab

local VisualsTab = Window:Tab("Visuals")

local OtherTab = Window:Tab("Other")

Window:AddSettingsTab()

Window:SwitchTab(LegitTab)

Window.ToggleAnime(false)

LoadTime = math.floor((tick() - LoadTime) * 1000)

]])
